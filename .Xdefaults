!!!!!!!! Muh(fia
URxvt.font: -windows-montecarlo-medium-r-normal--11-110-72-72-c-60-microsoft-cp1252

URxvt.boldFont: -windows-montecarlo-medium-r-normal--11-110-72-72-c-60-microsoft-cp1252


! cause shell to be started as login shell
URxvt*loginShell: true

! do not scroll with output
 URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true

!  retro scrollbar
URxvt.scrollBar: false
URxvt.scrollBar_right: true
URxvt.scrollstyle: xterm
URxvt.scrollColor: #000000

! clickable URLS please
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: chrome
URxvt.matcher.button: 1
URxvt.colorUL: #87FFAF

!Misc. settings, tired of explaining and talking to myself
URxvt.imLocale: en_US.UTF-8
URxvt.saveLines: 512
URxvt.colorBD: #857672
URxvt.colorIT: #bbbbbb
URxvt.cursorBlink: false
URxvt.cursorColor: #ff8939
URxvt.cursorColor2: #121212

!XFT settings
Xft.antialias: true
Xft.hinting: full
Xft.hintstyle: hintslight
Xft.rgba: rgb

!Color


!URxvt Colors
*foreground: #9C9A94
*background: #111111

!black
*color0:     #222222
*color8:     #404040
!red
*color1:     #DA5F5F
*color9:     #E17A6B
!green
*color2:     #95C749
*color10:    #B8D68C
!yellow
*color3:     #B5E15D
*color11:    #F0BB6E
!blue
*color4:     #778798
*color12:    #D2B48C
!magenta
*color5:     #AEA4BF
*color13:    #5AF4B5
!cyan
*color6:     #CCCA9A
*color14:    #42717B
!white
*color7:     #ECEAE0
*color15:    #dddddd

!Clock colors
XClock.Clock.majorColor:  #dddddd
XClock.Clock.minorColor:  #dddddd
XClock.Clock.hourColor:   #94C7B5
XClock.Clock.secondColor: #dddddd
XClock.Clock.minuteColor: #dddddd
